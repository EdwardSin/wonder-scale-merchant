{
  "name": "wonder-scale-merchant",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --port 4201 --ssl true --host 0.0.0.0 --disableHostCheck",
    "build": "ng build --configuration production --aot --output-hashing=all",
    "upload": "aws s3 cp dist/wonder-scale-merchant s3://merchant.wonderscale.com --recursive",
    "upload-testing": "aws s3 cp dist/wonder-scale-merchant s3://merchant-testing.wonderscale.com --recursive",
    "build-testing": "ng build --configuration qa --aot --output-hashing=all",
    "test": "ng test",
    "lint": "ng lint",
    "postinstall": "ngcc",
    "e2e": "ng e2e",
    "deploy": "npm run build && npm run upload",
    "deploy-testing": "ng build --configuration qa --aot && npm run upload-testing"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@angular/animations": "~12.2.2",
    "@angular/cdk": "^12.2.2",
    "@angular/common": "~12.2.2",
    "@angular/compiler": "~12.2.2",
    "@angular/core": "~12.2.2",
    "@angular/forms": "~12.2.2",
    "@angular/material": "^12.2.2",
    "@angular/material-moment-adapter": "^12.2.2",
    "@angular/platform-browser": "~12.2.2",
    "@angular/platform-browser-dynamic": "~12.2.2",
    "@angular/router": "~12.2.2",
    "@auth0/angular-jwt": "^4.0.0",
    "@types/googlemaps": "^3.39.3",
    "@types/heremaps": "^3.1.0",
    "@types/jquery": "^3.3.34",
    "angular-bootstrap-md": "^9.0.0",
    "angularx-social-login": "^4.0.1",
    "braintree-web": "^3.81.0",
    "chart.js": "^2.9.4",
    "croppie": "^2.6.4",
    "file-saver": "^2.0.2",
    "jquery": "^3.6.0",
    "jquery.qrcode": "^1.0.3",
    "jszip": "^3.7.1",
    "jszip-utils": "^0.1.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "ng-lazyload-image": "^7.1.0",
    "ng-mat-select-infinite-scroll": "^3.0.0",
    "ng2-charts": "^2.3.2",
    "ngx-cookie-service": "^3.0.4",
    "ngx-dropzone": "^2.2.2",
    "ngx-mat-select-search": "^3.3.0",
    "ngx-order-pipe": "^2.1.1",
    "ngx-progressbar": "^6.0.2",
    "ngx-sortablejs": "^3.1.4",
    "ngx-swiper-wrapper": "^10.0.0",
    "rxjs": "~6.5.4",
    "sortablejs": "^1.10.2",
    "tslib": "^2.0.0",
    "xlsx": "^0.17.0",
    "zone.js": "~0.11.4"
