{
    "name": "wonder-scale-merchant",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "dev": "ng serve --port 4201 --ssl true --host 0.0.0.0 --disableHostCheck",
        "build": "ng build --prod --aot --output-hashing=all",
        "upload": "aws s3 cp dist/wonder-scale-merchant s3://merchant.wonderscale.com --recursive",
        "deploy": "npm run build && npm run upload",
        "upload-testing": "aws s3 cp dist/wonder-scale-merchant s3://merchant-testing.wonderscale.com --recursive",
        "build-testing": "ng build --configuration qa --aot --output-hashing=all",
        "deploy-testing": "ng build --configuration qa --aot && npm run upload-testing",
        "test": "ng test",
        "lint": "ng lint",
        "postinstall": "ngcc",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@agm/core": "^1.1.0",
        "@angular/animations": "~9.0.2",
        "@angular/cdk": "^9.2.0",
        "@angular/common": "~9.0.2",
        "@angular/compiler": "~9.0.2",
        "@angular/core": "~9.0.2",
        "@angular/forms": "~9.0.2",
        "@angular/material": "^9.2.0",
        "@angular/platform-browser": "~9.0.2",
        "@angular/platform-browser-dynamic": "~9.0.2",
        "@angular/router": "~9.0.2",
        "@auth0/angular-jwt": "^4.0.0",
        "@types/googlemaps": "^3.39.3",
        "@types/heremaps": "^3.1.0",
        "@types/jquery": "^3.3.34",
        "angular-bootstrap-md": "^9.0.0",
        "angularx-social-login": "^3.0.1",
        "braintree-web": "^3.65.0",
        "chart.js": "^2.9.3",
        "croppie": "^2.6.4",
        "file-saver": "^2.0.2",
        "jquery": "^3.4.1",
        "jquery.qrcode": "^1.0.3",
        "jszip": "^3.5.0",
        "jszip-utils": "^0.1.0",
        "jwt-decode": "^2.2.0",
        "moment": "^2.24.0",
        "ng-lazyload-image": "^7.1.0",
        "ng2-charts": "^2.3.2",
        "ngx-cookie-service": "^3.0.4",
        "ngx-dropzone": "^2.2.2",
        "ngx-order-pipe": "^2.0.4",
        "ngx-progressbar": "^6.0.2",
        "ngx-sortablejs": "^3.1.4",
        "ngx-swiper-wrapper": "^9.0.1",
        "rxjs": "~6.5.4",
        "sortablejs": "^1.10.2",
        "tslib": "^1.10.0",
        "xlsx": "^0.16.1",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.900.3",
        "@angular/cli": "~9.0.3",
        "@angular/compiler-cli": "~9.0.2",
        "@angular/language-service": "~9.0.2",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "@types/sortablejs": "^1.10.5",
        "codelyzer": "^5.1.2",
        "ftp-deploy": "^2.3.8",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
